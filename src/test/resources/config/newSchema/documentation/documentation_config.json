                "submodelElements": [
                  {
                    "@foreach": {
                      "@xpath": "."
                    },
                    "@bind": {
                      "idShort": {
                        "@xpath": "'DocumentID'"
                      },
                      "value": {
                        "@xpath": "@ID"
                      }
                    },
                    "semanticId": {
                      "valueId": "http://www.sap.com/ain/documentation/document/@ID",
                      "keyType": "CUSTOM",
                      "keyElement": "SUBMODEL_ELEMENT"
                    },
                    "modelType": "Property",
                    "valueType": "string"
                  },
                  {
                    "@foreach": {
                      "@xpath": "."
