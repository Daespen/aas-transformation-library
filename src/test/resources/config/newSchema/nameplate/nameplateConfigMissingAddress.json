{
  "version": "2.0.0",
  "aasVersion": "3.0RC01",
  "config": [
    {
      "@header": {
        "@definitions": {
          "mappingIdGeneration": {
            "@concatenate_and_hash": [
              {
                "@xpath": "caex:Attribute[@Name='IdentificationData']/caex:Attribute[@Name='Manufacturer']/caex:Value"
              },
              {
                "@const": "DefaultManufacturer"
              },
              {
                "@const": "/"
              },
              {
                "@xpath": "caex:Attribute[@Name='IdentificationData']/caex:Attribute[@Name='ManufacturerUri']/caex:Value"
              }
            ]
          },
          "assetIdGeneration": {
            "@concatenate_and_hash": [
              {
                "@xpath": "caex:Attribute[@Name='IdentificationData']/caex:Attribute[@Name='Manufacturer']/caex:Value"
              },
              {
                "@const": "DefaultManufacturer"
              },
              {
                "@const": "/"
              },
              {
                "@xpath": "caex:Attribute[@Name='IdentificationData']/caex:Attribute[@Name='ManufacturerUri']/caex:Value"
              }
            ]
          }
        },
        "@variables": {
        },
        "@namespaces": {
          "ns1": "http://ns.org/1",
          "ns2": "http://ns.org/2",
          "caex": "http://www.dke.de/CAEX"
        }
      },
      "@mappings": {
        "@foreach": {
          "@xpath": "//caex:SystemUnitClass[caex:SupportedRoleClass/@RefRoleClassPath='AutomationMLComponentStandardRCL/AutomationComponent' or caex:RoleRequirements/@RefBaseRoleClassPath='AutomationMLComponentStandardRCL/AutomationComponent'] | //caex:InternalElement[caex:SupportedRoleClass/@RefRoleClassPath='AutomationMLComponentStandardRCL/AutomationComponent' or caex:RoleRequirements/@RefBaseRoleClassPath='AutomationMLComponentStandardRCL/AutomationComponent']"
        },
        "@bind": {
          "id": {
            "@def": "mappingIdGeneration"
          }
        },
        "assetInformation": {
          "@bind": {
            "id": {
              "@def": "assetIdGeneration"
            },
            "kindType": {
              "@xpath": "TYPE"
            }
          },
          "globalAssetIdReference": {
            "valueId": "assetInformationGlobalAssetIdReference",
            "keyType": "CUSTOM",
            "keyElement": "ASSET"
          }
        },
        "assetShell": {
          "id": "shellId",
          "@bind": {
            "idShort": {
              "@xpath": "@Name"
            }
          }
        },
        "submodels": [
          {
            "@foreach": {
              "@xpath": "caex:Attribute[@Name='CommercialData']/caex:Attribute[@Name='ManufacturerDetails']"
            },
            "semanticId": {
              "valueId": "https://admin-shell.io/zvei/nameplate/1/0/Nameplate",
              "keyType": "IRI",
              "keyElement": "SUBMODEL"
            },
            "@bind": {
              "id": {
                "@concatenate": [
                  {
                    "@def": "assetIdGeneration"
                  },
                  {
                    "@const": "_submodel1"
                  }
                ]
              }
            },
            "@foreach": {
              "@caexAttributeName": "Name"
            },
            "@bind": {
              "idShort": {
                "@xpath": "'Nameplate'"
              }
            },
            "submodelElements": [
              {
                "@foreach": {
                  "@caexAttributeName": "Name"
                },
                "@bind": {
                  "idShort": {
                    "@xpath": " 'ManufacturerName'"
                  },
                  "values": [
                    {
                      "lang": {
                        "@xpath": "'en-us'"
                      },
                      "value": {
                        "@xpath": "'caex:Value'"
                      }
                    }
                  ]
                },
                "semanticId": {
                  "valueId": "0173-1#02-AAO677#002",
                  "keyType": "IRDI",
                  "keyElement": "SUBMODEL_ELEMENT"
                },
                "modelType": "MultiLanguageProperty"
              },
              {
                "@foreach": {
                  "@xpath": "//caex:Attribute[@Name='DescriptionLong']"
                },
                "@bind": {
                  "idShort": {
                    "@xpath": " 'ManufacturerProductDesignation' "
                  },
                  "values": [
                    {
                      "lang": {
                        "@xpath": "'en-us'"
                      },
                      "value": {
                        "@xpath": "'caex:value'"
                      }
                    }
                  ]
                },
                "semanticId": {
                  "valueId": "0173-1#02-AAW338#001",
                  "keyType": "IRDI",
                  "keyElement": "SUBMODEL_ELEMENT"
                },
                "modelType": "MultiLanguageProperty"
              }
            ]
          }
        ]
      }
    }
  ]
}
