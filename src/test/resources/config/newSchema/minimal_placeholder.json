{
  "version": "1.0.0",
  "aasVersion": "3.0RC01",
  "config": [
    {
      "@header": {
        "@definitions": {
        },
        "@vars": {
          "assetName": "Name of the asset",
          "submodelName": "Name of the submodel"
        },
        "@namespaceBindings": {
          "ns1": "http://ns.org/1",
          "ns2": "http://ns.org/2",
          "caex": "http://www.dke.de/CAEX"
        }
      },
      "@mappings": {
        "@foreach": {
          "@xpath": "//caex:SystemUnitClass"
        },
        "submodels": [
          {
            "@foreach": {
              "@caexAttributeName": "IdentificationData"
            },
            "semanticId": "http://sap.com",
            "id": {
              "@concatenate": [
                {
                  "string": "submodel "
                },
                {
                  "var": "submodelName"
                },
                {
                  "string": " of asset "
                },
                {
                  "var": "assetName"
                }
              ]
            },
            "submodelElements": []
          }
        ],
        "assetInformation": {
          "@bind": {
            "kindType": {
              "@xpath": "'TYPE'"
            }
          },
          "id": "assetInformationId",
          "globalAssetIdReference": {
            "valueId": "assetInformationGlobalAssetIdReference",
            "keyType": "CUSTOM",
            "keyElement": "ASSET"
          }
        },
        "assetShell": {}
      }
    }
  ]
}
